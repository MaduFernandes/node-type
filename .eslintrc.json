{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
    },
    "collectCoverage": true,
    "collectCoverageFrom": ["<rootDir>/src/routes/**/*test.ts"],
    "coverageDirectory": "coverage",
    "coverageReporters": [
        "text-summary",
        "lcov"
    ]
}
